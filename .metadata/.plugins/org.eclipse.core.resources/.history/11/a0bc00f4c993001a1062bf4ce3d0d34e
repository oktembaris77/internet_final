package DAO;

import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.PreparedStatement;

import Classes.TAKIM;

public class TakimKayitIslemi {
	String connURL = "jdbc:sqlserver://localhost:1433:databaseName=DB_TURNUVA;integratedSecurity=true";
	public int TakimKayit(TAKIM takim) {
		String insertQuery = "INSERT INTO takimlar VALUES(?,?)";
		try {
			Connection conn = DriverManager.getConnection(connURL);
			PreparedStatement pst = conn.prepareStatement(insertQuery);
		}
	}
}
